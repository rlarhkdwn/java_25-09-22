package day17;

import java.io.FileReader;

public class File03 {

	public static void main(String[] args) {
		// 파일 읽ㄱ
		// 보조 스트림 사용 BufferedRead (FileReader)
		// 라인 단위르 일기 가능
		// 더 이상 읽을 라인이 없으면 null;
		// 기반 스트림을 매개변수로 포함해야함
		 
		BufferedRaader br = new BufferedReader(new FileReader("test.txt"));

	}

}
