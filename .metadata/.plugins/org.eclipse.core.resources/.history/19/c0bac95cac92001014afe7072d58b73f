package day16;

import java.util.HashMap;
import java.util.Map;

public class Lambda {

	public static void main(String[] args) {
		Map<String, Integer> map = new HashMap<>();
		map.put("hong", 89);
		map.put("kim", 96);
		map.put("lee", 68);
		map.put("choi", 45);
		map.put("park", 78);
		
		// int sum = 0;
		map.forEach((a, b) -> {
			// sum += b; : 불가
			System.out.println(a+":"+b);
		});
		
		
		Number sum = (a, b) -> {
			return a + b;
		};
		
		Number max = (a, b) -> {
			return a >= b ? a : b; 
		};
		
		
		int s = sum.add(10,  20);
		System.out.println(s);
		System.out.println(max.add(10, 20));
	}
	
	// 람다용 함수형 인터페이스 생성
	// 메서드는 반드시 1개여야만 함
	
}

// 람다용 함수형 인터페이스 생성
// 메서드는 반드시 1개여야만 함
@FunctionalInterface
interface Number {
	int add(int a, int b);
	// int max(int a, int b); // 메서드가 2개면 error
}